{
  "name": "gflow",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx src/server/index.ts",
    "dev:server:new": "tsx src/server/index.new.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "dev:all:new": "concurrently \"npm run dev\" \"npm run dev:server:new\"",
    "build": "vite build",
    "preview": "vite preview",
    "server": "tsx src/server/index.ts",
    "server:new": "tsx src/server/index.new.ts",
    "db:migrate": "tsx -e \"import('./src/server/db').then(m => m.createDatabaseFromEnv().then(db => db.migrate().then(() => db.close())))\""
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@vitejs/plugin-react": "^5.1.1",
    "axios": "^1.13.2",
    "better-sqlite3": "^11.6.0",
    "pg": "^8.13.1",
    "body-parser": "^2.2.1",
    "cors": "^2.8.5",
    "dagre": "^0.8.5",
    "express": "^5.1.0",
    "fs-extra": "^11.1.1",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.554.0",
    "markdown-to-jsx": "^7.1.9",
    "node-cron": "^4.2.1",
    "path": "^0.12.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reactflow": "^11.11.4",
    "url": "^0.11.4",
    "vite": "^7.2.4",
    "vm": "^0.1.0",
    "vm2": "^3.10.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.11",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^22.14.0",
    "@types/pg": "^8.11.10",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^8.2.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
